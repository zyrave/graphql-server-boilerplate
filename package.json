{
  "name": "type-graphql-boilerplate",
  "version": "1.0.0",
  "description": "TypeGraphQL Boilerplate",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "tsc && cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpileOnly src/index.ts",
    "docker:start": "tsc && docker-compose -f docker-compose.yml up --build -d",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:build": "tsc && docker build --rm -t type-graphql-boilerplate-server .",
    "docker:stop": "docker-compose down",
    "clean": "rm -rf dist node_modules",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dbtest:setup": "ts-node-dev src/utils/test/setup.ts",
    "test": "yarn dbtest:setup && jest"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.10.0",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "graphql": "^14.5.6",
    "graphql-upload": "^8.0.7",
    "ioredis": "^4.14.1",
    "nodemailer": "^6.3.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.19",
    "typeorm-typedi-extensions": "^0.2.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.0.1",
    "@types/connect-redis": "^0.0.11",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.0.17",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/nodemailer": "^6.2.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.3"
  }
}
