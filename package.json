{
  "name": "type-graphql-boilerplate",
  "version": "1.0.0",
  "description": "TypeGraphQL Boilerplate",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "docker:start": "tsc && yarn clean:data && docker-compose -f docker-compose.yml up --build -d",
    "docker:dev": "yarn clean:data && docker-compose -f docker-compose.dev.yml up --build",
    "docker:build": "tsc && yarn clean:data && docker build -t type-graphql-server .",
    "docker:stop": "docker-compose down",
    "clean": "rm -rf dist node_modules",
    "clean:data": "sudo rm -fr data/postgres data/redis && mkdir -p data/postgres data/redis",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dbtest:setup": "ts-node-dev src/test-utils/setup.ts",
    "test": "yarn dbtest:setup && jest"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.10.0",
    "connect-redis": "^4.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "graphql": "^14.5.4",
    "ioredis": "^4.14.0",
    "nodemailer": "^6.3.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.11",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.0.17",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/nodemailer": "^6.2.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.2"
  }
}
